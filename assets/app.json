{
  "name": "postnotification",
  "app_services": {
    "NotifyService": {
      "fields": {
        "queue": "backend.Queue",
        "storageService": "postnotification.StorageService"
      },
      "services": [
        "StorageService"
      ],
      "databases": [
        "notification_queue"
      ]
    },
    "StorageService": {
      "fields": {
        "cache": "backend.Cache"
      },
      "databases": [
        "post_db"
      ]
    },
    "UploadService": {
      "fields": {
        "notifyService": "postnotification.NotifyService",
        "queue": "backend.Queue",
        "storageService": "postnotification.StorageService"
      },
      "services": [
        "StorageService",
        "NotifyService"
      ],
      "databases": [
        "notification_queue"
      ]
    }
  },
  "app_databases": {
    "notification_queue": {
      "type": "Queue"
    },
    "post_db": {
      "type": "Cache"
    }
  }
}